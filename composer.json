{
    "name": "cybercog/laravel-love",
    "description": "Make Laravel Eloquent models Likeable & Dislikeable in a minutes!",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "cybercog",
        "cog",
        "laravel",
        "eloquent",
        "trait",
        "love",
        "hate",
        "likeable",
        "likable",
        "like",
        "dislike",
        "favorite",
        "favourite",
        "rate",
        "rating",
        "emotion",
        "reaction",
        "star",
        "kudos",
        "adore",
        "unlike",
        "undislike"
    ],
    "authors": [
        {
            "name": "Anton Komarev",
            "email": "a.komarev@cybercog.su",
            "homepage": "http://ell.folio.su",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/cybercog/laravel-love",
    "support": {
        "email": "open@cybercog.su",
        "issues": "https://github.com/cybercog/laravel-love/issues",
        "wiki": "https://github.com/cybercog/laravel-love/wiki",
        "source": "https://github.com/cybercog/laravel-love",
        "docs": "https://github.com/cybercog/laravel-love/wiki"
    },
    "require": {
        "php": "^7.0",
        "illuminate/database": "~5.5.0|~5.6.0",
        "illuminate/support": "~5.5.0|~5.6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.10",
        "mockery/mockery": "^1.0",
        "orchestra/database": "~3.5.0|~3.6.0",
        "orchestra/testbench": "~3.5.0|~3.6.0",
        "phpunit/phpunit": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Cog\\Contracts\\Love\\": "contracts/",
            "Cog\\Laravel\\Love\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cog\\Tests\\Laravel\\Love\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cog\\Laravel\\Love\\Providers\\LoveServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable" : true
}
