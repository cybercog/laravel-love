{
    "name": "cybercog/laravel-likeable",
    "description": "Trait for Laravel Eloquent models to allow easy implementation of a `like` & `dislike` features.",
    "keywords": [
        "trait",
        "laravel",
        "eloquent",
        "likeable",
        "likable",
        "like",
        "dislike",
        "favorite",
        "favourite",
        "rate",
        "rating"
    ],
    "authors": [
        {
            "name": "Anton Komarev",
            "email": "a.komarev@cybercog.su",
            "homepage": "http://ell.folio.su",
            "role": "Developer"
        }
    ],
    "license": "BSD-3-Clause",
    "type": "library",
    "require": {
        "php": "^5.6|^7.0",
        "illuminate/database": "~5.1.20|~5.2.0|~5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2",
        "orchestra/testbench": "~3.0",
        "mockery/mockery": "~0.9",
        "friendsofphp/php-cs-fixer": "^1.11"
    },
    "autoload": {
        "psr-4": {
            "Cog\\Likeable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cog\\Likeable\\Tests\\": "tests/"
        }
    }
}
